<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<shortcuts>
    <skipIfNotSupported/>
    <defaultCurrentUser/>
    <programGroup defaultName="AgileSites2" location="applications"/>
    <shortcut KdeUsername="root" applications="no"
        commandLine="sites-server.bat"
        description="WebCenter Sites with AgileSites" desktop="yes"
        encoding="UTF-8" initialState="normal" name="Sites"
        programGroup="yes" startMenu="no" startup="no"
        target="$INSTALL_PATH/sites-server.bat" type="Application" workingDirectory="$INSTALL_PATH"/>
    <shortcut KdeUsername="root" applications="no"
        description="WebCenter Sites with debugging" desktop="yes"
        encoding="UTF-8" initialState="normal" name="Sites (debug)"
        programGroup="yes" startMenu="no" startup="no"
        target="$INSTALL_PATH/sites-server-debug.bat" type="Application" workingDirectory="$INSTALL_PATH"/>
</shortcuts>
