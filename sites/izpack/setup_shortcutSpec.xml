<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="AgileSites2" location="applications"/>
    <shortcut applications="no" commandLine="run"
        description="WebCenter Sites with AgileSites" desktop="yes"
        initialState="normal" name="Sites" programGroup="yes"
        startMenu="no" startup="no"
        target="$INSTALL_PATH/sites-server.cmd" workingDirectory="$INSTALL_PATH"/>
    <shortcut applications="no" commandLine="debug"
        description="Sites with Debugging" desktop="yes"
        initialState="noShow" name="Sites (debug)" programGroup="yes"
        startMenu="no" startup="no"
        target="$INSTALL_PATH/sites-server.cmd" workingDirectory="$INSTALL_PATH"/>
</shortcuts>
