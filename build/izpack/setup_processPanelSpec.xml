<processing>
    <logfiledir>$INSTALL_PATH/</logfiledir>
    <job name="Setup.bat">
        <os family="Windows"/>
        <executefile name="$INSTALL_PATH/setup/setup.bat">
            <arg>$JAVA_HOME</arg>
            <arg>$INSTALL_PATH</arg>
        </executefile>
    </job>
    <job name="Setup.sh">
        <os family="Unix"/>
        <executefile name="$INSTALL_PATH/setup/setup.sh">
            <arg>"$JAVA_HOME" "$INSTALL_PATH"</arg>
        </executefile>
    </job>
</processing>
